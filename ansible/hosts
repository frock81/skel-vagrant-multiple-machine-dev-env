[nodeX_servers]
# Ansible ssh host is needed since the controller cant resolve.
node01.localdomain ansible_ssh_host=192.168.4.
node02.localdomain ansible_ssh_host=192.168.4.

[nodeX_servers:vars]
ansible_ssh_private_key_file=/vagrant/insecure_private_key

[controller]
controller01 ansible_connection=local